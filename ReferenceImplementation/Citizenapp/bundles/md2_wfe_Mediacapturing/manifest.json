{
    "Bundle-SymbolicName": "md2_wfe_Mediacapturing",
    "Bundle-Version": "2.0",
    "Bundle-Name": "Workflow Element Mediacapturing",
    "Bundle-Description": "Generated MD2 workflow element bundle: Mediacapturing of Citizenapp",
    "Bundle-Localization": [],
    "Bundle-Main": "",
    "Require-Bundle": [],
    "Require-Bundle": [
        {
            "name": "md2_runtime"
        },
        {
            "name": "md2_models"
        },
        {
            "name": "md2_content_providers"
        },
        {
            "name": "md2_workflow"
        }
    ],
    "Components": [
        {
            "name": "MD2MediaCapturing",
            "impl": "ct/Stateful",
            "provides": ["md2.wfe.MediaCapturing.AppDefinition"],
            "propertiesConstructor": true,
            "properties": {
                "appId": "md2_MediaCapturing",
                "id": "md2_MediaCapturing",
                "windowTitle": "MediaCapturing",
                "onInitialized": "__startupAction",
                "views": [
                    {
                        "name": "MediaCapturingView",
                        "dataForm": {
                            "dataform-version": "1.0.0",
                            "type": "md2gridpanel",
                            "cols": "1",
                            "valueClass": "layoutCell",
                            "cellStyle": {
                                "width": "100%"
                            }
                            ,
                            "children": [
                                {
                                    "type": "textoutput",
                                    "datatype": "string",
                                    "field": "TitleLbl2",
                                    "defaultText": "Upload Photo",
                                    "cellStyle": {
                                        "width": "100%"
                                    }
                                },
                                {
                                    "type": "button",
                                    "title": "Upload File",
                                    "field": "UploadBtn",
                                    "cellStyle": {
                                        "width": "100%"
                                    }
                                },
                                {
                                    "type": "simpleimage",
                                    "src": "md2_app_ReferenceProject:./resources/defaultImage.png",
                                    "cellStyle": {
                                        "width": "100%"
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        },
        {
            "name": "CustomActions",
            "provides": ["md2.wfe.MediaCapturing.CustomActions"],
            "instanceFactory": true
        },
        {
            "name": "Controller",
            "provides": ["md2.wfe.MediaCapturing.Controller", "md2.app.Citizenapp.controllers"],
            "instanceFactory": true,
            "references": [
                {
                    "name": "_md2AppWidget",
                    "providing": "md2.runtime.InstanceFactory"
                },
                {
                    "name": "_customActions",
                    "providing": "md2.wfe.MediaCapturing.CustomActions"
                },
                {
                    "name": "_models",
                    "providing": "md2.app.Citizenapp.Models"
                },
                {
                    "name": "_contentProviders",
                    "providing": "md2.app.Citizenapp.ContentProvider",
                    "cardinality": "0..n"
                },
                {
                    "name": "_configBean",
                    "providing": "md2.wfe.MediaCapturing.AppDefinition"
                },
                {
                    "name": "_workflowEventHandler",
                    "providing": "md2.workflow.EventHandler"
                },
				{
                    "name": "_workflowStateHandler",
                    "providing": "md2.workflow.WorkflowStateHandler"
                }
            ]
        },
        {
            "name": "MD2MediaCapturingTool",
            "impl": "ct.tools.Tool",
            "provides": ["ct.tools.Tool"],
            "propertiesConstructor": true,
            "properties": {
                "id": "md2_app_MediaCapturing_tool",
                "title": "[MediaCapturing]",
                "description": "[MediaCapturing]",
                "tooltip": "[MediaCapturing]",
                "toolRole": "toolset",
                "iconClass": "icon-view-grid",
                "togglable": true,
                "activateHandler": "startWorkflow",
                "deactivateHandler": "closeWindow"
            },
            "references": [
                {
                    "name": "handlerScope",
                    "providing": "md2.wfe.MediaCapturing.Controller"
                }
            ]
